#!/usr/bin/env node

// Execute
const path = require("path");
const child = require("child_process");
const fiftPath = path.resolve(__dirname, "exec", "func");
const [, , ...argv] = process.argv;
try {
  child.execSync(fiftPath + " " + argv.join(" "), {
    stdio: "inherit",
  });
} catch (e) {
  process.exit(1);
}
